{
    "presets": [
        "@babel/preset-react",
        ["@babel/preset-env", {
            "debug"           : false,
            "modules"         : false,
            "shippedProposals": true,
            "useBuiltIns"     : false,
            "targets"         : { "browsers": ["last 3 versions", "ie >= 11"] }
        }]
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose" : true }]
    ]
}